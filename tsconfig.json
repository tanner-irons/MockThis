{
    "compilerOptions": {
      "rootDir": ".",
      "outDir": "dist",
      "target": "es2019",
      "lib": [
        "es2019",
        "dom"
      ],
      "module": "commonjs",
      "esModuleInterop": true,
      "strict": true,
      "sourceMap": true,
      "allowJs": true,
      "noImplicitAny": false,
      "skipLibCheck": true,
      "strictNullChecks": false
    },
    "exclude": [
      "node_modules"
    ]
}